import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int p=sc.nextInt();
         int b=p/10;
        int c=p%10;
        int d=b+c;
        System.out.printf("Bird said:%d\n",p);
        System.out.printf("Alice must go in path-%d",d);
    }
}
