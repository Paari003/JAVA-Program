import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int count=0,sum=0,rem;
        System.out.println("The Count the digits: "+count(num));
       }
    static int count(int n)
    {
        if(n<10){
            return 1;
        }
       return 1+count(n/10);
    }
}
